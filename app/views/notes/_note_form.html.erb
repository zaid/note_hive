<%= form_for [notebook, note] do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
  <div class='field'>
    <%= f.text_area :content, :class => 'note_content_textarea' %>
  </div>
  <div class='field'>
    <%= f.label 'Tags' %>
    <%= f.text_field :tag_list, :size => '90' %>
  </div>
  <div class='actions'>
    <%= f.submit %>
  </div>
<% end %>
